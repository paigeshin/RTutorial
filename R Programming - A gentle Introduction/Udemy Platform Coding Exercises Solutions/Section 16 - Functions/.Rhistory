extract_by_brand = function("brand") {
data(mtcars)
mtcars$brand = rownames(mtcars)
list_cars = strsplit(mtcars$brand, ' ')
vector_cars = sapply(list_cars, '[', 1)
mtcars$brand = vector_cars
mtcars[mtcars$brand == "brand",]
}
